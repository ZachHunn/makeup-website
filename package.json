{
  "name": "makeup-website",
  "version": "0.1.0",
  "quokka": {
    "babel": true
  },
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watchAll",
    "test:commit": "jest",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost:3000 cypress run",
    "e2e:pipeline": "cypress run",
    "db:studio": "dotenv -e .env.development -- npx prisma studio",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset dev",
    "db:seed": "dotenv -e .env.development -- npx prisma db seed",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@nextui-org/react": "^1.0.0-beta.10",
    "@prisma/client": "^4.8.1",
    "@react-aria/ssr": "^3.4.1",
    "@supabase/auth-helpers-nextjs": "^0.5.2",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.2.6",
    "@supabase/supabase-js": "^2.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.2.2",
    "babel-plugin-macros": "^3.1.0",
    "dayjs": "^1.11.7",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "next": "^13.1.2",
    "notistack": "^2.0.8",
    "prettier": "^2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-query": "^3.39.2",
    "square": "^24.0.0",
    "twin.macro": "^3.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hookform/resolvers": "^2.9.10",
    "@testing-library/dom": "^8.19.1",
    "@types/node": "^18.11.18",
    "@types/react-datetime-picker": "^3.4.1",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.3.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prisma": "^4.8.1",
    "react-hook-form": "^7.42.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
